parameters:
    paths:
        - %currentWorkingDirectory%/
    excludes_analyse:
        - %currentWorkingDirectory%/vendor/
        - %currentWorkingDirectory%/assets/
    scanDirectories:
        - ../woocommerce/
        - ../../../wp-includes/
        - ../../../wp-admin/
#        - ../woocommerce-subscriptions
#        - ../wordpress-develop-tests
    level: 0
    checkMissingIterableValueType: false
    ignoreErrors:
        - "#Inner named functions are not supported by PHPStan#"
        - "#Function wcs_order_contains_renewal not found.#"
        - "#unknown class WC_Subscriptions_Renewal_Order#"
